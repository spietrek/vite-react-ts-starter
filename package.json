{
  "name": "vite-react-ts-starter",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "spietrek"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "watch:tailwind": "npx tailwindcss -i ./src/styles/index.css -o ./app/styles/index.css --watch",
    "type:check": "tsc",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .ts,.tsx --ignore-path .gitignore src",
    "lintfix": "eslint --ext .ts,.tsx --ignore-path .gitignore --fix src",
    "build:css": "postcss src/styles/index.css -o app/styles/index.css --env production",
    "preview:css": "postcss src/styles/index.css -o app/assets/index.css",
    "build:run": "yarn build:tailwind && yarn build",
    "build:deptree": "depcruise -x '^node_modules' --output-type dot src | dot -T svg > dependencygraph.svg",
    "check:dupecode": "jscpd src",
    "pwa:test": "serve -s dist -l 60562"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.12",
    "@reduxjs/toolkit": "^1.8.6",
    "axios": "^1.1.3",
    "clsx": "^1.2.1",
    "daisyui": "^2.31.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-ga": "^3.3.1",
    "react-intl": "^6.2.1",
    "react-quill": "^2.0.0",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "web-vitals": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/react": "^6.5.12",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.0",
    "@types/react": "~18.0.21",
    "@types/react-dom": "~18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "cssnano": "^5.1.13",
    "dependency-cruiser": "^11.17.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "6.1.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.7.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "jest-watch-typeahead": "^2.2.0",
    "jscpd": "^3.5.0",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "postcss-nested": "^6.0.0",
    "postcss-nested-ancestors": "^3.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-pwa": "^0.13.1",
    "vite-tsconfig-paths": "^3.5.1",
    "workbox-window": "^6.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version",
      "last 2 edge version"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  }
}
